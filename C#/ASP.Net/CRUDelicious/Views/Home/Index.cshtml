@{
    ViewData["Title"] = "Home Page";
}
@model Dish;

<div class="text-center">
    <h1 class="display-4">Welcome to CRUDelicious</h1>
    <p>Check out some recent dishes!<a href="/NewDish">"Add a Dish!"</a></p>

    <ul style = "list-style-type:none;">@{
                    foreach(var d in ViewBag.All)
                    {
                        <li><a href="View/@d.DishId">@d.DishName</a> by @d.ChefName</li>
                    }
                }
    </ul>
</div>
