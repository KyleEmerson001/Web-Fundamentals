<h1>@ViewData["Title"]</h1>
@model LoginUser

<h1 class="display-4">Login to this Cool Site!</h1>
<form asp-controller="Home" asp-action="Login" method="POST">
    <div class="form-group">
        <label asp-for="LoginEmail"></label>
        <span asp-validation-for="LoginEmail" class="text-danger"></span>
        <input asp-for="LoginEmail" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="LoginPassword"></label>
        <span asp-validation-for="LoginPassword" class="text-danger"></span>
        <input asp-for="LoginPassword" class="form-control">
    </div>
    <input id="generate" type="Submit" value ="Login">
</form>